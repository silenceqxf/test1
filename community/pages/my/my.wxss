/* pages/my/my.wxss */
page{
  background-color:#f0f0f0;
}
.enter{
  height:180rpx;
  display:flex;
  align-items:center;
  padding:0 30rpx;
  margin-bottom:15rpx;
  background-color:#54afc5;
} 
.avatar{
  padding-right:40rpx;
  display:flex;
  align-items:center;
} 
.avatar image{
  width:120rpx;
  height:120rpx;
  border-radius:50%;
}
.info{
  width:400rpx;
  height:100rpx;
  display:flex;
  flex-direction:column;
  padding-top:30rpx;
} 
.name,.address{
  width:100%;
  height:50rpx;
  display:flex;
}
.name{
  margin-bottom:10rpx;
}
.info .user{
  font-size:26rpx;
}

.main{
  display:flex;
  margin:20rpx 0;
  flex-direction:column;
  background-color:white;
}
.myItems{
  width:100%;
  height:100rpx;
  display:flex;
  border-bottom:1rpx solid #f0f0f0;
  margin-left:30rpx;
  align-items:center;
}
.myItems .word{
  width:560rpx;
}
.myItems .word1{
  width:200rpx;
}
.myItems .iconfont{
  font-size:38rpx;
  padding-right:30rpx;
  color: #54afc5;
}
navigator .dibudaohanglan- {
  color: #54afc5;
  font-size:45rpx;
}
.service{
  background-color:white;
}
.my_suggests,
.contact_service {
  border: none;
}
.phone{
  margin-left:160rpx;
}
